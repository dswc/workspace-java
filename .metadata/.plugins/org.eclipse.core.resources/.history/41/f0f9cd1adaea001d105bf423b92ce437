import java.util.Arrays;

public class test2 {
    public void run() {
        int[] arr = { 11, 2, 3, 14, 5, 16, 7 };

        int min = Integer.MAX_VALUE;

        for (int i = 0; i < arr.length; i++) {
            if (min > arr[i])
                min = arr[i];
        }

//        Arrays.sort(arr);
