
public class test {
    public static void main(String[] args) {
        Baby tom = new Baby(50, 3500);
        
        Baby smallTom = tom;

        System.out.println(smallTom.height);
    }
}

class Baby {
    int height;
    int weight;
    String sex;
    
    public Baby(int h) {
        height = h;
    }
    
    public Baby(int h, int w) {
        height = h;
        weight = w;
    }
}

