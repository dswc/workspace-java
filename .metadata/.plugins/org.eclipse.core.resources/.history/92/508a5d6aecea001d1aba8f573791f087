class Baby {
    int height;
    int weight;
    String sex;

    public Baby(int height) {
        this(height, 3500);
    }

    public Baby(int h, int w) {
        height = h;
        weight = w;
    }

    public void sleep() {
        System.out.println("sleep......");
    }
}
